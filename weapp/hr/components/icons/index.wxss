@font-face {
    font-family: "i-weapp";
  	src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2')
}

.i-weapp {
    display: inline-block;
    font-family: i-weapp;
    speak: none;
    font-style: normal;
    font-weight: 400;
    font-variant: normal;
    text-transform: none;
    text-rendering: auto;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    vertical-align: middle
}
.i-pwd_show:before { content: "\e600"; }

.i-pwd_hide:before { content: "\e601"; }

.i-woman:before { content: "\e602"; }

.i-man:before { content: "\e603"; }

.i-iphone:before { content: "\e604"; }

.i-more:before { content: "\e605"; }

.i-switch:before { content: "\e606"; }

.i-warn:before { content: "\e607"; }

.i-weixin:before { content: "\e608"; }

.i-delete:before { content: "\e609"; }

.i-search:before { content: "\e60a"; }

.i-company:before { content: "\e60b"; }

.i-download:before { content: "\e60c"; }
.i-download{
    background-image: linear-gradient(23deg, #8749FE 2%, #BF8FFF 64%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
.i-forward:before { content: "\e60d"; }
.i-forward{
    background-image: linear-gradient(222deg, #6AF1C7 0%, #3D75F3 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
.i-logout:before { content: "\e60e"; }
.i-logout{
    background-image: linear-gradient(217deg, #FAD961 0%, #F76B1C 90%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
.i-star_all:before { content: "\e60f"; }

.i-star_right:before { content: "\e610"; }

.i-msg_fill:before { content: "\e611"; }

.i-msg:before { content: "\e612"; }

.i-query:before {
  content: "\e613";
}
.i-query{
    background-image: linear-gradient(-42deg, #ED2E18 0%, #F76B1C 91%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
